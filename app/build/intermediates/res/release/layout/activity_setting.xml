<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="com.wjkj.kd.teacher.SettingActivity">

    <ScrollView
        android:id="@+id/myScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="match_parent">



    <RelativeLayout
        android:background="#FF3366"
        android:id="@+id/rl_finish"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="设置"
            android:id="@+id/textView8"
            android:textSize="24sp"
            style="@style/MyTextViewStyle"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/imageView8"

            android:src="@drawable/zuojiantou2x"
            android:layout_marginLeft="27dp"
            style="@style/MyTextViewStyle"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="27dp" />
    </RelativeLayout>


        <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        style="@style/MyLineStyle"
        android:id="@+id/textView12"
        android:layout_below="@+id/r2_push"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        style="@style/MyLineStyle"
        android:id="@+id/textView13"
        android:layout_below="@+id/r3_aboutus"
        android:layout_centerHorizontal="true" />

            <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        style="@style/MyLineStyle"
        android:id="@+id/textView15"
        android:layout_below="@+id/r4_fankui"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        style="@style/MyLineStyle"
        android:id="@+id/textView17"
        android:layout_below="@+id/r5_update"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

            <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#bbbbbb"

        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/tv_update"
        android:layout_below="@+id/r6_cacle"
        android:layout_centerHorizontal="true" />

        </LinearLayout>

    </ScrollView>

    <RelativeLayout
        android:id="@+id/r6_cacle"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_below="@+id/textView27"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="注销"
            android:layout_marginLeft="30dp"
            style="@style/MyTextViewStyle"
            android:id="@+id/tv_cancle"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/r5_update"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_below="@+id/textView26"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="版本更新"
            android:layout_marginLeft="30dp"
